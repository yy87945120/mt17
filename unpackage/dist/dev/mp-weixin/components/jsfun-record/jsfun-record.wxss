.jsfun-record .mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
}
.jsfun-record .conbox {
  -webkit-transition: all .3s ease;
  -o-transition: all .3s ease;
  transition: all .3s ease;
  -webkit-transform: translateY(100%);
      -ms-transform: translateY(100%);
          transform: translateY(100%);
  position: fixed;
  z-index: 1000;
  right: 0;
  left: 0;
  bottom: 0;
  background: #fff;
}
.jsfun-record .conbox.pickerShow {
    -webkit-transform: translateY(0);
        -ms-transform: translateY(0);
            transform: translateY(0);
}
.jsfun-record .c666 {
  color: #666;
}
.jsfun-record .c999 {
  color: #999;
}
.jsfun-record .fz28 {
  font-size: 28rpx;
}
.jsfun-record .fz32 {
  font-size: 32rpx;
}
.jsfun-record .record {
  text-align: center;
}
.jsfun-record .record .time {
    text-align: center;
    font-size: 60rpx;
    color: #000;
    line-height: 100rpx;
    margin-top: 50rpx;
}
.jsfun-record .record .domess {
    margin-bottom: 50rpx;
}
.jsfun-record .record .record-box {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
        -ms-flex-pack: center;
            justify-content: center;
}
.jsfun-record .record canvas {
    margin: 10rpx 60rpx;
    position: relative;
    width: 200rpx;
    height: 200rpx;
    z-index: 10;
}
.jsfun-record .record canvas .recording {
      position: absolute;
      top: 20rpx;
      left: 20rpx;
      width: 160rpx;
      height: 160rpx;
      border: 1px dashed #fe3b54;
      border-radius: 100rpx;
      background: #fe3b54 url(data:image/png;base64,) no-repeat 50% 50%;
      background-size: 50% 50%;
      z-index: 100;
}
.jsfun-record .record .btncom, .jsfun-record .record .stop, .jsfun-record .record .paly, .jsfun-record .record .confirm {
    margin-top: 70rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 80rpx;
}
.jsfun-record .record .stop {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
.jsfun-record .record .paly {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
.jsfun-record .record .confirm {
    background: url(data:image/png;base64,) no-repeat 100% 100%;
    background-size: 100% 100%;
}

